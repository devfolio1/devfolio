{
    "version": 2,
    "builds": [
      {
        "src": "./app.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/build",    
        "use": "@vercel/static"
      }
    ],
  
   
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "./app.js"
      }
    ]
  }